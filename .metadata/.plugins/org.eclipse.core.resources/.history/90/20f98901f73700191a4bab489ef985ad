import java.util.Arrays;

public class Solution {
	public static void main(String[] args) {
		int toys = 7;
		int money = 50;
		int a[] = {1,12,5,111,200,1000,10};
		int temp[] =new int[a.length];
		int sum =0;
		Arrays.sort(a);
		for (int i = 0; i < a.length; i++) {
			if(a[i]<money)
			{
				temp[i]=a[i];
			}
			else
			{
				break;
			}
		}
		for (int i = 0; i < temp.length; i++) {
			sum=sum+temp[i];
			
			if(sum<money)
			{
				count++;
			}
		}
		
	}
}
